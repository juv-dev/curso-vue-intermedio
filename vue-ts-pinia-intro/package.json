{
  "name": "piniaapp",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev:backend": "json-server --watch db/db.json --port 3001 --no-cors --static ./"
  },
  "dependencies": {
    "@tanstack/vue-query": "5.92.5",
    "axios": "1.13.5",
    "pinia": "3.0.4",
    "vue": "3.5.26",
    "vue-router": "4.6.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.15.0",
    "@types/json-server": "0.14.8",
    "@types/node": "25.0.3",
    "@vitejs/plugin-vue": "6.0.3",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.39.2",
    "eslint-plugin-vue": "10.6.2",
    "json-server": "0.17.4",
    "npm-run-all": "4.1.5",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vue-tsc": "3.2.1"
  }
}